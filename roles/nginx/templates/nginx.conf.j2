worker_processes auto;

events{
    worker_connections 1024;
}

http{
    include /conf/mime.types;
    include /etc/nginx/proxy;
    index index.html index.htm index.php;
    sendfile on;
    tcp_nopush on;
    root /usr/local/go-web;
    
    upstream go-web{
          server ;
          server ;
    }
    
    server{
           listen 80;
           server_name ;
           location /{
                  proxy_pass http://go-web;
           }
    
    
    }

}
